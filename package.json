		"eslint": "9.27.0",
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-svelte": "3.9.0",
		"globals": "16.2.0",
		"npm-check": "6.0.1",
		"prettier": "3.5.3",
		"prettier-plugin-svelte": "3.4.0",
		"svelte": "5.33.2",
		"svelte-check": "4.2.1",
		"typescript": "5.8.3",
		"typescript-eslint": "8.32.1",
